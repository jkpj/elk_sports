<h1><%= @series.race.name %></h1>
<h2><%= @series.name %> - Lähtölista</h2>
<table>
  <tr>
    <th>Nro</th>
    <th>Kilpailija</th>
    <th><%= t "attributes.club" %></th>
    <th>Lähtöaika</th>
  </tr>
  <% @series.start_list.each do |comp| %>
    <tr>
      <td><%= comp.number %></td>
      <td><%= full_name(comp) %></td>
      <td><%= comp.club.name %></td>
      <td><%= comp.start_time.strftime('%H:%M:%S') %></td>
    </tr>
  <% end %>
</table>
<br/>
<%= link_to "Tulokset", race_series_path(@series.race.id, @series) %>