<% if @club.destroyed? %>
  $("#action_error").removeClass("error");
  $("#action_error").html("");
  $("#club_row_<%= @club.id %>").attr('style', 'display: none;');
<% else %>
  $("#action_error").addClass("error");
  $("#action_error").html("Virhe poistettaessa seuraa!");
<% end %>
$(".spinner").parent().html("");