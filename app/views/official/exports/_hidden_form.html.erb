<%= f.hidden_field :sport_id %>
<%= f.hidden_field :location %>
<%= f.hidden_field :start_date %>
<%= f.hidden_field :end_date %>
<%= f.hidden_field :start_interval_seconds %>
<%= f.hidden_field :finished %>
<%= f.hidden_field :series_count %>
<%= f.hidden_field :team_competitor_count %>
<%= f.fields_for(:clubs) do |club_f| %>
  <%= club_f.hidden_field :id, :value => '' %>
  <%= club_f.hidden_field :name %>
<% end %>
<%= f.fields_for(:correct_estimates) do |club_f| %>
  <%= club_f.hidden_field :id, :value => '' %>
  <%= club_f.hidden_field :min_number %>
  <%= club_f.hidden_field :max_number %>
  <%= club_f.hidden_field :distance1 %>
  <%= club_f.hidden_field :distance2 %>
  <%= club_f.hidden_field :distance3 %>
  <%= club_f.hidden_field :distance4 %>
<% end %>
<%= f.fields_for(:series) do |series_f| %>
  <%= series_f.hidden_field :id, :value => '' %>
  <%= series_f.hidden_field :name %>
  <%= series_f.hidden_field :start_time %>
  <%= series_f.hidden_field :first_number %>
  <%= series_f.hidden_field :has_start_list %>
  <%= series_f.hidden_field :competitors_count %>
  <%= series_f.hidden_field :start_day %>
  <%= series_f.hidden_field :no_time_points %>
  <%= series_f.hidden_field :estimates %>
  <%= series_f.fields_for(:age_groups) do |age_group_f| %>
    <%= age_group_f.hidden_field :id, :value => '' %>
    <%= age_group_f.hidden_field :name %>
    <%= age_group_f.hidden_field :min_competitors %>
  <% end %>
  <%= series_f.fields_for(:competitors) do |competitor_f| %>
    <%= competitor_f.hidden_field :id, :value => '' %>
    <%= competitor_f.hidden_field :club_name, :value => competitor_f.object.club.name %>
    <% if competitor_f.object.age_group %>
      <%= competitor_f.hidden_field :age_group_name,
        :value => competitor_f.object.age_group.name %>
    <% end %>
    <%= competitor_f.hidden_field :first_name %>
    <%= competitor_f.hidden_field :last_name %>
    <%= competitor_f.hidden_field :number %>
    <%= competitor_f.hidden_field :start_time %>
    <%= competitor_f.hidden_field :arrival_time %>
    <%= competitor_f.hidden_field :shots_total_input %>
    <%= competitor_f.hidden_field :estimate1 %>
    <%= competitor_f.hidden_field :estimate2 %>
    <%= competitor_f.hidden_field :estimate3 %>
    <%= competitor_f.hidden_field :estimate4 %>
    <%= competitor_f.hidden_field :no_result_reason %>
    <%= competitor_f.hidden_field :correct_estimate1 %>
    <%= competitor_f.hidden_field :correct_estimate2 %>
    <%= competitor_f.hidden_field :correct_estimate3 %>
    <%= competitor_f.hidden_field :correct_estimate4 %>
    <%= competitor_f.fields_for(:shots) do |shot_f| %>
      <%= shot_f.hidden_field :id, :value => '' %>
      <%= shot_f.hidden_field :value %>
    <% end %>
  <% end %>
<% end %>